generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User{
  id            String    @id @default(uuid())
  firstName     String
  lastName      String? 
  email         String    @unique 
  clearkUserId  String?   @unique 
  imageUrl      String? 
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt

  @@map("users")
}